  },
  "dependencies": {
    "@mantine/core": "7.1.7",
    "@mantine/hooks": "7.1.7",
    "@types/node": "20.8.10",
    "@types/react-dom": "18.2.14",
    "next": "14.0.1",
    "react-dom": "18.2.0",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.7.2",
    "@types/node": "20.2.5",
    "@types/react-dom": "18.2.4",
    "@types/react": "18.2.34",
    "eslint-config-next": "14.0.1",
    "eslint-plugin-next-on-pages": "^1.7.2",
    "eslint": "8.53.0",
    "postcss-preset-mantine": "1.9.0",
    "postcss-simple-vars": "^7.0.1",
    "postcss": "^8.4.24",
    "typescript": "5.0.4",
    "vercel": "^32.5.0",
    "wrangler": "^3.15.0"
  }
